<?xml version="1.0" encoding="UTF-8"?>
<configuration debug="false">
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d [%20thread] %-5level %-24logger{0} - %msg%n</pattern>
        </encoder>
    </appender>

    <!--<logger name="org.chesscorp" level="DEBUG"/>-->
    <!--<logger name="org.chesscorp.club.service.ChessPositionServiceImpl" level="TRACE"/>-->

    <root level="OFF">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>
